{
  "name": "vue-notus",
  "homepage": "https://demos.creative-tim.com/vue-notus/",
  "version": "1.1.0",
  "description": "Vue Notus - A free Tailwind CSS and VueJS UI Kit and Admin by Creative Tim.",
  "repository": "https://github.com/creativetimofficial/vue-notus",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider PUBLIC_URL=/ CI=false vue-cli-service build && gulp licenses",
    "lint": "vue-cli-service lint",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@popperjs/core": "2.9.1",
    "@primevue/themes": "^4.1.0",
    "@tailwindcss/forms": "0.2.1",
    "@vee-validate/rules": "^4.13.2",
    "@vue/compiler-sfc": "3.0.7",
    "axios": "^0.21.4",
    "bootstrap": "^5.3.0",
    "bootstrap-vue-next": "^0.24.23",
    "canvas-confetti": "^1.9.3",
    "chart.js": "2.9.4",
    "core-js": "3.9.1",
    "element-plus": "^2.8.4",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "he": "^1.2.0",
    "install": "^0.13.0",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "npm": "^10.9.0",
    "pdfjs-dist": "^4.6.82",
    "pdfmake": "^0.2.13",
    "primevue": "^4.1.0",
    "simple-vue-validator": "^0.16.0",
    "vee-validate": "^4.13.2",
    "vue": "^3.5.10",
    "vue-awesome-paginate": "^1.1.46",
    "vue-datepicker-next": "^1.0.3",
    "vue-loading-overlay": "^4.0.4",
    "vue-router": "4.0.5",
    "vue-sweetalert2": "^5.0.11",
    "vue3-editor": "^0.1.1",
    "vue3-tel-input": "^1.0.4",
    "vue3-toastify": "^0.2.3",
    "vue3-treeselect": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/eslint-parser": "7.13.10",
    "@vue/cli-plugin-babel": "5.0.0-alpha.7",
    "@vue/cli-plugin-eslint": "5.0.0-alpha.7",
    "@vue/cli-service": "5.0.0-alpha.7",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "7.22.0",
    "eslint-plugin-vue": "7.7.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
